import java.util.Vector;

public class VectorGrowth {
    public static void main(String[] args) {

        // Initial capacity = 10
        Vector<String> vector = new Vector<>(10);

        // Add 10 elements
        for (int i = 1; i <= 10; i++) {
            vector.add("IT2300" + i);
        }

        System.out.println("Initial Capacity of Vector: " + vector.capacity());

        // Exceed initial capacity
        vector.add("IT23011");

        System.out.println("Capacity After Exceeding: " + vector.capacity());
        System.out.println("Current Size: " + vector.size());
    }
}
